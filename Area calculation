#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      int a,b,c;
    float d;  
    scanf("%d%d%d%f",&a,&b,&c,&d);
    int r1=a*a;
    printf("%d\n",r1);
    int r2=b*c;
    printf("%d\n",r2);
    float r3=3.14*d*d;
    printf("%.2f\n",r3);
    return 0;
}
