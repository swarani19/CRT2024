#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int num,temp,rem,sum=0,n;
    scanf("%d",&num);
    temp=num;
    n=log10(temp)+1;
    while(num>0)
    {
        rem=num%10;
        sum+=pow(rem,n);
        num=num/10;
    }
    if(temp==sum)
    {
        printf("Yes");
    }
    else
    {
        printf("No");
    }    
    return 0;
}
