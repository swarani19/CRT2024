#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    char str[100];
    scanf("%[^\n]s",str);
    int i;
    for(i=0;str[i]!='\0';i++)
    {
        if(str[i]>=90 && str[i]<=122)
        {
            str[i]=str[i]-32;
        }
    }
    printf("%s",str);
    return 0;
}
