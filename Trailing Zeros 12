#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
    int fac=1,count=0;
    for(int i=1;i<=n;i++)
    {
        fac=fac*i;
    }
    while(fac!=0)
    {
        int r=fac%10;
        fac=fac/10;
        if(r==0)
        {
            count++;
        }
        else
        {
            break;
        }
    }
    printf("%d",count);
    return 0;
}
