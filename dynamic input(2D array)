#include <stdio.h>
int main() 
{
    int a[2][2];
    int i,j;
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        scanf("%d",&a[i][j]);
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            for(j=0;j<2;j++)
            printf("%d ",a[i][j]);
            printf("\n");
        }
    }
    
    return 0;
}

output:
4
1
6
2
4 1 
6 2 
