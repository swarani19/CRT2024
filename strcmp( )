#include <stdio.h>
#include<string.h>
int main() {
    char str1[50],str2[50];
    scanf("%s",str1);
    scanf("%s",str2);
    int i=strcmp(str1,str2);
    if(i==0)
    {
        printf("Strings are equal");
    }
    else
    {
        printf("Strings are not equal");
    }
   return 0;
}


output:
abc
abc
Strings are equal
